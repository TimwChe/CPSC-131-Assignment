#include <iostream>
#include <memory>
#include <vector>
using namespace std;

class Stack{
public:
Stack(){
    vector={};
}

void push(int data){
    vector.push_back(data);
}

int getSize(){
    return vector.size();
}

void pop(){
    int size = getSize();
    if(size==0){
        cout<<"Stack is empty"<<endl;
        return;
    }
    vector.pop_back();
}

void peek(){
    int size = getSize();
    if(size==0){
        cout<<"Stack is empty"<<endl;
        return;
    }
    cout<<vector[size-1]<<endl;
}

void isEmpty(){
    if(size==0){
        cout<<"Stack is empty";
    }
    return;
}

void isBalanced(string para){
    for(int n=0; n < para.size(); n++){
        if(para[n] == "(") {
            push(para[n]);
            return;
        }
        else{
            if(para[n] == ")"){
                if(vector.empty()){
                    cout<<"Unbalanced";
                    return;
                }
                else{
                    pop();
                    return;
                }
            }
        }
    }
    if(vector.empty()){
    cout<<"Balanced";
    return;
    }
    else{
        cout<<"Unbalanced";
        return;
    }
}

private:
vector<string> vector;
};